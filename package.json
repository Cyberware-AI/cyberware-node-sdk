    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "nock": "^13.5.4",
    "node-telegram-bot-api": "^0.66.0",
    "prettier": "^3.3.2",
    "ts-jest": "^29.1.5",
